<div class="panel panel-default">
  <div class="panel-body">

  <h2>Job Results:</h2>
  <hr>

  <% @jobs.each do |j| %>
  <div class="panel panel-default">
    <div class="panel-body">
      <h3><%= link_to j.title, job_path(j.id) %></h3>
    </div>
  </div>
  <% end %>
  <br>
  <h2>User Results:</h2>
  <hr>

  <% @users.each do |u| %>
  <div class="panel panel-default">
    <div class="panel-body">
      <h3><%= link_to u.full_name, user_profile_path(u.id) %></h3>
    </div>
  </div>
  <% end %>

  </div>
</div>
