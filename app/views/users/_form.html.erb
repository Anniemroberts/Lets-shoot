<%= @user.errors.full_messages.join(', ') %>
<%= simple_form_for @user do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :phone %>
  <%= f.input :bio %>
  <%= f.input :experience %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :image %>
  <%= f.submit 'Submit' %>
<% end %>
